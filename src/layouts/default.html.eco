<!DOCTYPE html>
<html lang="en">
<head>

  <!-- Standard Meta -->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width">

  <!-- Site Properities -->
  <title><%= @getPreparedTitle() %></title>
  <meta name="author" content="<%= @getPreparedAuthor() %>">
  <meta name="email" content="<%= @getPreparedEmail() %>" />
  <meta name="description" content="<%= @getPreparedDescription() %>">
  <meta name="keywords" content="<%= @getPreparedKeywords() %>">

  <!-- DocPad Meta -->
  <%- @getBlock('meta').toHTML() %>

  <!-- DocPad Styles + Our Own -->
  <%- @getBlock('styles').add(["/styles/jhtwbsstyles.css"]).add(@site.styles).toHTML() %>

<style>

</style>

  <!-- HTML5 shiv/shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
    <script src="/components/html5shiv/dist/html5shiv.js"></script>
    <script src="/components/respond/respond.min.js"></script>
  <![endif]-->

  <!-- Favicons -->
  <link rel="apple-touch-icon-precomposed" sizes="144x144" href="/icons/apple-touch-icon-144x144-precomposed.png">
  <link rel="apple-touch-icon-precomposed" sizes="114x114" href="/icons/apple-touch-icon-114x114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="/icons/apple-touch-icon-72x72-precomposed.png">
                  <link rel="apple-touch-icon-precomposed" href="/icons/apple-touch-icon-57x57-precomposed.png">
                                 <link rel="shortcut icon" href="/icons/favicon.png">

</head>

<body>
  <div id="top-banner" class="accent-row"></div>
  <div role="main">

    <!-- header
    ================================================== -->
    <header class="jh-heading" id="masthead">
      <div class="container">
        <div class="heading-content">

          <a href="/" class="jh-brand" title="Return Home">
            Jason Huntington
          </a>
          <img src="/img/jason-large.jpg" alt=" " class="heading-avatar" id="jason-photo">

          <p class="lead">
            Just an average guy experimenting with web development.<br>
            Available for consulting. <a href="mailto:jason@jasonhuntington.com" title="Contact Me" class="contact-button">Contact</a>
          </p>

        </div> <!-- /heading-content -->
      </div>
    </header>

    <div class="line"></div>

    <div class="jh-navbar">
      <div class="container">
        <nav class="mainnav">
          <ul class="nav">
            <% for document in @getCollection('navpages').toJSON(): %>
              <li typeof="sioc:Page" about="<%= document.url %>" class="<%= 'active'  if @document.url is document.url %>">
                <a href="<%= document.url %>" property="dc:title">
                  <i class="<%= document.navicon %>"></i>
                  <%= document.navlinktitle %>
                </a>
              </li>
            <% end %>
          </ul>
        </nav>
      </div>
    </div> <!-- /jh-navbar -->


    <div class="jh-container container">
      <section id="content" class="content">
        <%- @content %>
      </section>
    </div> <!-- /container -->

  </div> <!-- /main -->


  <aside class="social-connections">
    <div class="jh-container container">
      <h1>Connections</h1>

      <%- @partial('social.html') %>



    </div>

  </aside>

  <!-- colophon - main page footer (social buttons, copyright, etc) -->
  <footer class="footer colophon">
    <div class="jh-container container">
      <div class="about">
        <p>
          This website was created with <a href="http://bevry.me" target="_blank" title="Visit Website">Bevry's</a> <a href="http://docpad.org" target="_blank" title="Visit on GitHub">DocPad</a> Powered by <a href="http://nodejs.org/" target="_blank" title="Node.js">Node.js</a> and is <a href="http://github.com/jhuntdog/jasonhuntington.site" title="Github Source">Open Source</a>.
        </p>
      </div>
      <div class="copyright">
        <p>
          Unless stated otherwise, Copyright Â© 2013+ <a href="http://jasonhuntington.com">Jason Huntington</a>.
        </p>
        <p>
          Code licensed <a href="http://en.wikipedia.org/wiki/Permissive_free_software_licence">permissively</a> under the <a href="http://creativecommons.org/licenses/MIT/">MIT License</a> and <a href="http://creativecommons.org/licenses/by/3.0/">Creative Commons Attribution 3.0 Unported License</a> for everything else (including content, media, design). Enjoy!
        </p>
      </div>
    </div>
  </footer>

  <!-- Core JavaScript
  ================================================== -->
  <!-- Placed at the end of the document so the pages load faster -->

  <!-- jQuery -->
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
  <script>window.jQuery || document.write('<script src="/components/jquery/jquery.js"><\/script>')</script>

  <!-- bootstrap scripts -->
  <script src="/components/jhtwbs/docs/assets/js/bootstrap.js"></script>

  <!-- DocPad Scripts + Our Own -->
  <%- @getBlock('scripts').add(@site.scripts).toHTML() %>

  <!-- DocPad Scripts + Our Own -->
  <%- @getBlock('scripts').add(@site.scripts).add(["/scripts/scripts.js"].concat(@document.scripts or [])).toHTML() %>


</body>
</html>